<?xml version="1.0" encoding="UTF-8"?>

<element>
	<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xmlns="http://java.sun.com/xml/ns/javaee"
		xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
		id="WebApp_ID" version="2.5">

		
		
		<!-- gayashan -->
		
		<display-name>User Management Web Application</display-name>

		<error-page>
			<exception-type>java.lang.Exception</exception-type>
			<location>/Gayashan_Erorr.jsp</location>
		</error-page>

		<servlet>
       		<servlet-name>login</servlet-name>
         	<servlet-class>gayashan.Gayashan_LoginServelet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>login</servlet-name>
         	<url-pattern>/login</url-pattern>
         </servlet-mapping> 
         
         <servlet>
         		<servlet-name>deliveryInsert</servlet-name>
         		<servlet-class>art_Gallery.DeliveryInsert</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>deliveryInsert</servlet-name>
         	<url-pattern>/Deliveyinsert</url-pattern>
         </servlet-mapping>
         
         <servlet>
         	<servlet-name>DeleteDelivery</servlet-name>
         	<servlet-class>art_Gallery.DeliveryDeleteServelet</servlet-class>
         </servlet>
         
         <servlet-mapping>
         	<servlet-name>DeleteDelivery</servlet-name>
         	<url-pattern>/delete</url-pattern>
         </servlet-mapping>
         
		<!-- gayashanEnd -->
    
    
    
	</web-app>
</element>